{
	"name": "Free Code Camp Side Panel",
  "version": "0.1",
  "manifest_version": 3,
	"description": "Uses side panel to provide easy access to FreeCodeCamp's curriculum.",
	"icons": {
		"256": "images/fcc_extension_logo.png"
	},
	"side_panel": {
		"default_path": "sidePanel.html"
	},
	"permissions": ["sidePanel", "webNavigation", "tabs", "tabGroups"],
	"action": {
    "default_title": "Click to open panel"
  },
	"background": {
		"service_worker": "service-worker.js",
		"type": "module"
	}
}